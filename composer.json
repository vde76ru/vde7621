{
    "name": "vdestor/site",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "WaySen",
            "email": "vde76ru@yandex.ru"
        }
    ],
    "require": {
        "opensearch-project/opensearch-php": "^2.4"
    },
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@composer dump-autoload -o"
        ]
    }
}
